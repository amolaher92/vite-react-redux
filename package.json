    "eslint-plugin-react-refresh": "^0.4.16",
    "globals": "^15.14.0",
    "jest": "^29.7.0",
    "jsdom": "^25.0.1",
    "prettier": "^3.4.2",
    "ts-jest": "^29.2.5",
    "typescript": "~5.6.2",
